import maya.cmds as cmds

animCameraShape = '?NS?igCam_Animation'
cmds.lookThru(animCameraShape)

    
def getShotFromCamera(camera):
    
    def getShotInfo(shotNode):
        startFrame = cmds.shot(shotNode,q = True,startTime = True)
        endFrame = cmds.shot(shotNode,q = True,endTime = True)
        camera = cmds.shot(shotNode,q = True,currentCamera = True)
        return startFrame,endFrame,camera
    
    shots = cmds.ls(type = 'shot')
    for shot in shots:
        info = getShotInfo(shot)
        camNS = camera.split(':')[0]
        shotCamNS = info[2].split(':')[0]
        if camNS == shotCamNS:
            return shot,info[0],info[1]
    
shotInfo = getShotFromCamera(animCameraShape)
cmds.playbackOptions(e = 1, ast = shotInfo[1])
cmds.playbackOptions(e = 1, aet = shotInfo[2])
cmds.playbackOptions(e = 1, min = shotInfo[1])
cmds.playbackOptions(e = 1, max = shotInfo[2])
cmds.currentTime(shotInfo[1])   