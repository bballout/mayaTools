# Import the module
import subprocess

process = subprocess.Popen([r'C:\Users\bballout\Desktop\test.bat'],shell = True,stdin=subprocess.PIPE,stdout=subprocess.PIPE,stderr = subprocess.STDOUT)
grep_stdout = process.communicate(input='echo c')[0]
print(grep_stdout.decode())


