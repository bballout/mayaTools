chunkInt = [4, 5, 10, 8 , 1, 3, 16, 17, 21, 22, 23]
geo = []

for i in chunkInt:
    geo.append('road_chunk%i'%i)
    
import maya.cmds as cmds
cmds.select(geo)

sel = cmds.ls(sl = True)

for obj in sel:
    cmds.select(cl = True)
    joint = cmds.joint()
    constraint = cmds.parentConstraint(obj, joint)
    cmds.delete(constraint) 